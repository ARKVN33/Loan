        <Name>PInsTotalAmount</Name>
        <NameInSource>RepAllPer</NameInSource>
        <Parameters isList="true" count="0" />
        <SqlCommand>SELECT ISNULL(SUM(CAST(InstallmentDueAmount AS BIGINT)), 0) AS InsTotalAmount
FROM (SELECT dbo.tblLoan.Loan_Personnel_Id, dbo.tblInstallment.InstallmentAmount, dbo.tblInstallment.InstallmentDueAmount
