          <value>Loan_LoanType_Id,System.Byte</value>
          <value>LoanInstallmentNum,System.Byte</value>
          <value>Id,System.Int32</value>
        </Columns>
        <CommandTimeout>30</CommandTimeout>
        <Dictionary isRef="1" />
        <Name>AllLoan</Name>
        <NameInSource>RepLoan</NameInSource>
        <Parameters isList="true" count="0" />
        <SqlCommand>SELECT dbo.tblWage.WageAmount, dbo.tblLoan.LoanAmount, dbo.tblLoan.LoanDate,
  dbo.tblLoan.LoanPayOff, dbo.tblInfo.InfoFirstName, dbo.tblInfo.InfoLastName,
  dbo.tblPersonnel.PersonnelId, dbo.tblWage.Wage_WageType_Id,
  dbo.tblInfo.InfoNationalCode, dbo.tblLoan.Loan_LoanType_Id,
  dbo.tblLoan.LoanInstallmentNum, dbo.tblLoan.Id
FROM dbo.tblWage INNER JOIN
  dbo.tblLoan ON dbo.tblWage.Wage_Loan_Id = dbo.tblLoan.Id INNER JOIN
  dbo.tblPersonnel ON dbo.tblLoan.Loan_Personnel_Id = dbo.tblPersonnel.Id
  INNER JOIN
  dbo.tblInfo ON dbo.tblPersonnel.Personnel_Info_Id = dbo.tblInfo.Id
WHERE tblPersonnel.Id = {PerId} AND tblLoan.Id = {LoanId}</SqlCommand>
      </AllLoan>
